{
    "name": "serendipity-swow/serendipity-job",
    "license": "MIT",
    "keywords": [
        "swow",
        "coroutine",
        "php8",
        "swoole",
        "job",
        "nsq"
    ],
    "authors": [
        {
            "name": "codingheping",
            "email": "847050412@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Serendipity\\Job\\": "src"
        },
        "files": [
            "src/Kernel/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-bcmath": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-redis": ">=5.3",
        "ext-sockets": "*",
        "guzzlehttp/guzzle": "^7.3",
        "hyperf/contract": "2.2.*",
        "hyperf/di": "2.2.*",
        "hyperf/engine-swow": "^1.2",
        "hyperf/pool": "2.2.*",
        "hyperf/utils": "2.2.*",
        "monolog/monolog": "^2.2",
        "multiplex/multiplex": "^0.3.1",
        "nesbot/carbon": "^2.49",
        "nette/utils": "^3.2",
        "nikic/fast-route": "^1.3",
        "serendipity-swow/archer": "^0.2.0",
        "serendipity-swow/nsq": "0.8",
        "serendipity-swow/sockets": "0.8",
        "spatie/emoji": "^2.3",
        "swow/swow": "dev-develop",
        "symfony/config": "^5.3",
        "symfony/console": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/serializer-pack": "^1.0",
        "symfony/yaml": "^5.2",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/var-dumper": "^5.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "phpunit --prepend tests/bootstrap.php -c phpunit.xml --colors=always",
        "cs-fix": [ "@php vendor/bin/php-cs-fixer fix" ],
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config"
    },
    "bin": [
        "bin/serendipity-job"
    ]
}
