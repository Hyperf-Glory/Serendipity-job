{
    "name": "serendipity/serendipity-job",
    "license": "MIT",
    "keywords": [
        "swow",
        "coroutine",
        "php8",
        "swoole"
    ],
    "authors": [
        {
            "name": "codingheping",
            "email": "847050412@qq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Serendipity\\Job\\": "src"
        },
        "files": [
            "src/Kernel/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require": {
        "php":">=8.0",
        "ext-redis":">=5.3",
        "ext-sockets": "*",
        "symfony/console": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/yaml": "^5.2",
        "monolog/monolog": "^2.2",
        "swow/swow": "dev-develop",
        "nette/utils": "^3.2",
        "vlucas/phpdotenv": "^5.3",
        "symfony/config": "^5.2",
        "ext-iconv": "*",
        "hyperf/engine-swow": "^1.1",
        "ext-pdo": "*",
        "symfony/serializer-pack": "^1.0",
        "multiplex/multiplex": "^0.3.1",
        "ext-bcmath": "*",
        "hyperf/di": "2.2.x-dev",
        "serendipity-swow/sockets": "^0.2.0",
        "nesbot/carbon": "^2.49",
        "hyperf/pool": "^2.1",
        "hyperf/redis": "^2.1",
        "guzzlehttp/guzzle": "^7.3",
        "serendipity-swow/nsq": "^0.5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "symfony/var-dumper": "^5.2"
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "cs-fix": [ "@php vendor/bin/php-cs-fixer fix" ],
        "analyse": "phpstan analyse --memory-limit 300M -l 0 -c phpstan.neon ./app ./config"
    },
    "bin": [
        "bin/serendipity-job"
    ]
}
